{
	"port":  80,

	"default": "martens.ms",

	"hosts": [


		[[
			"martens.ms",
			"www.martens.ms",
			"christoph-martens.de",
			"www.christoph-martens.de"
		], {
			"root": "%root%/external/martens.ms",
			"redirects": {
				"/lycheeJS/docs/getting-started.html": "http://lycheejs.org/docs/index.html",
				"/lycheeJS/*": "http://lycheejs.org/*",
				"/lycheeJS": "http://lycheejs.org/index.html"
			}
		}],

		[[
			"lycheejs.org",
			"www.lycheejs.org"
		], {
			"root": "%root%/external/lycheejs.org"
		}],

		[ "forge.lycheejs.org", {
			"root": "%root%"
		}],

		[ "cosmo.lycheejs.org", {
			"root": "%root%/game/cosmo"
		}],

		[ "jewelz.lycheejs.org", {
			"root": "%root%/game/jewelz",
			"aliases": {
				"backend": "%root%/forge/backend"
			}
		}]

	]
}
